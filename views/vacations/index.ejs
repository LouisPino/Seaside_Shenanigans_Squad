<%- include('../partials/header')%>

<table id="list">
    <thead>
      <tr>
        <th>Location</th>
        <th>Dates</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% movies.forEach(function(m) { %>
        <tr>
          <td><%= m.title %></td>
          <td><%= m.releaseYear %></td>
          <td><a href="/movies/<%= m._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table><p>        <% vacations.forEach(f=>{ %>
    <div><%= f.location %></div>
    <div><%= f.arrival %></div>
    <div><%= f.departure %></div>
  <% }) %></p>

<%- include('../partials/footer')%>