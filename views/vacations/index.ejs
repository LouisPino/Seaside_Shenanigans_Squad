<%- include('../partials/header')%>

<table id="list">
    <thead>
      <tr>
        <th>Location</th>
        <th>Dates</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
        <% vacations.forEach(f=>{ %>
            <tr>
                <td><%= f.location %></td>
                <td><%= f.arrival.toLocaleDateString() %></td>
                <td><%= f.departure.toLocaleDateString() %></td>
                <td><a href="/vacations/<%= f._id %>">Details</a></td>
            </tr>
        <% }) %>
      </tbody>
    </table>

<%- include('../partials/footer')%>